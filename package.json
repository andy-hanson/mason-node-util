{
	"name": "mason-node-util",
	"version": "0.0.0",
	"files": ["dist", "src"],
	"repository": "mason-lang/mason-node-util",
	"license": "WTFPL",
	"scripts": {
		"build": "babel src --out-dir dist --source-maps",
		"watch": "babel src --out-dir dist --source-maps --watch",
		"lint": "eslint src"
	},
	"dependencies": {
		"chalk": "1.1.1",
		"mason-compile": "mason-lang/mason-compile",
		"source-map-support": "0.3.3"
	},
	"devDependencies": {
		"babel-cli": "6.0.14",
		"babel-plugin-transform-es2015-destructuring": "6.0.14",
		"babel-plugin-transform-es2015-modules-umd": "6.0.14",
		"babel-plugin-transform-es2015-parameters": "6.0.14",
		"babel-plugin-transform-strict-mode": "6.0.14",
		"eslint": "1.10.1",
		"eslint-config-mason": "mason-lang/eslint-config-mason"
	},
	"babel": {
		"plugins": [
			"transform-es2015-destructuring",
			"transform-es2015-modules-umd",
			"transform-es2015-parameters",
			"transform-strict-mode"
		]
	},
	"eslintConfig": {"extends": "eslint-config-mason"}
}
