{
	"name": "mason-node-util",
	"version": "0.0.0",
	"files": ["dist", "src"],
	"license": "Unlicense",
	"repository": "mason-lang/mason-node-util",
	"scripts": {
		"build": "babel src --out-dir dist --source-maps",
		"watch": "babel src --out-dir dist --source-maps --watch",
		"lint": "eslint src"
	},
	"dependencies": {
		"chalk": "1.1.1",
		"source-map-support": "0.4.0"
	},
	"devDependencies": {
		"babel-cli": "6.3.17",
		"babel-plugin-transform-es2015-destructuring": "6.3.15",
		"babel-plugin-transform-es2015-modules-umd": "6.3.13",
		"babel-plugin-transform-es2015-parameters": "6.3.26",
		"babel-plugin-transform-strict-mode": "6.3.13",
		"eslint": "2.0.0-alpha-2",
		"eslint-config-mason": "mason-lang/eslint-config-mason"
	},
	"babel": {
		"plugins": [
			"transform-es2015-destructuring",
			"transform-es2015-modules-umd",
			"transform-es2015-parameters",
			"transform-strict-mode"
		]
	},
	"eslintConfig": {"extends": "eslint-config-mason"}
}
